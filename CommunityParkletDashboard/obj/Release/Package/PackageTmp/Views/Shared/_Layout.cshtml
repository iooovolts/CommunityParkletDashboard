<!DOCTYPE html>
<html>
<head>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <title>Community Parklet Application Dashboard</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="style.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/jquery.slick/1.5.7/slick.css" />
    <link rel="stylesheet" type="text/css" media="screen" href="http://www.hackney.gov.uk/Style/notifier-bar-lbh.css" />

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>
    <script src="http://www.hackney.gov.uk/resource/themes/hackney/scripts/hackney.js"></script>
    <script src="http://www.hackney.gov.uk/resource/themes/hackney/scripts/lazyload.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/jquery.slick/1.5.7/slick.min.js"></script>
    <script type="text/javascript" src="http://www.hackney.gov.uk/Script/jquery.cookie.js"></script>
    <script type="text/javascript" src="http://www.hackney.gov.uk/Script/jquery.lbh.consent-cookie.js"></script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-2561609-1', 'auto');
        ga('send', 'pageview');
    </script>
</head>

<body id="landing-template" class="js">
    <div class="responsive"></div>

    <header class="no-print">
        <div class="headerBanner" style="display:block">


            <div class="site-banner" id="site-banner" role="banner"></div>
            <div class="mobUtilities">
                <div class="top-utilities">


                    <ul class="utilities inline-list" role="navigation">
                        <li>@Html.ActionLink("Community Parklet Applications", "CPDashboard", "CPDashboard")</li>
                        <li>@Html.ActionLink("Community Parklet Emails", "CPEmailDashboard", "CPDashboard")</li>
                    </ul>
                </div>
            </div>

        </div>
    </header>
    <div class="sectionone">
        <div class="headerLogo">

            <a id="skipNav" href="#skipNavigation">Skip Navigation</a>
            <div class="wrapper">

                <div class="site-logo" role="banner">
                    <a href="http://www.hackney.gov.uk/index" title="Link to homepage" accesskey="1">
                        <img src="http://www.hackney.gov.uk/resource/themes/hackney/images/Hackney_Logo_White.png" alt="Hackney" />
                    </a>
                    <div role="navigation" class="burger">Menu<img class="burgermenu" alt="menu image" src="http://www.hackney.gov.uk/resource/themes/hackney/images/burger.png"></div>
                </div>

                <div class="top-utilities">


                    <ul class="utilities inline-list" role="navigation">
                        <li>@Html.ActionLink("Community Parklet Applications", "CPDashboard", "CPDashboard")</li>
                        <li>@Html.ActionLink("Community Parklet Emails", "CPEmailDashboard", "CPDashboard")</li>
                    </ul>
                </div>

            </div>
        </div>

        <div class="article-heading" id="skipNavigation">
            <div class="wrapper">
                <h1 style="font-size:1.71em; text-align:left;padding-left:10.5em">
                    Community Parklet Application
                    <!--application name-->
                </h1>

            </div>
        </div>




        <div class="wrapper">

            <div class="main">

                <a id="SkipNavigation" class="accessibility"></a>
                <main>

                    <article>



                        <div class="textblock-default">
                            @*CONTENT HERE*@
                            @RenderBody()
                        </div>




                        <script type="text/javascript">
                            var screenwidth = 0;
                            screenwidth = $(window).width();

                            $(window).resize(function () { //on window resize
                                //check not ie7/ie8...
                                if (jQuery.support.opacity != false) {
                                    if (screenwidth != $(window).width()) { //only check for horizontal resize
                                        landnavigation(); //call nav
                                    }
                                }
                            });

                            landnavigation(); //call nav on page load

                            function landnavigation() {
                                //<![CDATA[
                                jQuery(function ($) {

                                    if ($('#landing-template .landing-top-panel').css('z-index') == '1') {
                                        // Add some border styling
                                        $(".landing-top-panel li span").css('border-right', '3px solid #BAE2A4');
                                        $(".landing-top-panel li span").css('border-bottom', '3px solid #BAE2A4');
                                        $(".landing-top-panel li:nth-child(2n) span").css('border-right', 'none');
                                        $(".landing-top-panel li:nth-child(n+7) span").css('border-bottom', 'none');
                                    }
                                    else {
                                        // Add some border styling
                                        $(".landing-top-panel li span").css('border-right', '3px solid #BAE2A4');
                                        $(".landing-top-panel li span").css('border-bottom', '3px solid #BAE2A4');
                                        $(".landing-top-panel li:nth-child(4n) span").css('border-right', 'none');
                                        $(".landing-top-panel li:nth-child(n+5) span").css('border-bottom', 'none');
                                    }
                                });
                                //]]>
                            }

                        </script>



                    </article>
                </main>
            </div>
        </div>
        <footer class="no-print text-center">
            <div class="socialcontainer">
                <ul class="utilities" role="contentinfo">
                    <li><a href="http://www.hackney.gov.uk/accessibility-help">Accessibility </a></li>
                    <li><a href="http://www.hackney.gov.uk/apps">Apps</a></li>
                    <li><a href="http://www.hackney.gov.uk/a-to-z">A to Z</a></li>
                    <li><a href="http://www.hackney.gov.uk/contact-us">Contact us</a></li>
                    <li><a href="http://www.hackney.gov.uk/email-alerts">Email alerts</a></li>
                    <li><a href="http://www.hackney.gov.uk/help">Help</a></li>
                    <li><a href="http://www.hackney.gov.uk/News">News</a></li>
                    <li><a href="http://www.hackney.gov.uk/privacy">Privacy statement</a></li>
                    <li><a href="http://www.hackney.gov.uk/disclaimer">Terms and conditions</a></li>
                </ul>


                <ul class="icons">
                    <li>
                        <a href="http://www.twitter.com/hackneycouncil">
                            <img class="normal" src="http://www.hackney.gov.uk/media/7074/Twitter/normal/twitter" alt="Twitter" />
                            <img class="on-hover" src="http://www.hackney.gov.uk/media/7074/Twitter/highlight/twitter-hov" alt="Twitter" />
                        </a>
                    </li>
                    <li>
                        <a href="http://www.facebook.com/hackneycouncil">
                            <img class="normal" src="http://www.hackney.gov.uk/media/7072/Facebook/normal/facebook" alt="Facebook" />
                            <img class="on-hover" src="http://www.hackney.gov.uk/media/7072/Facebook/highlight/facebook-hov" alt="Facebook" />
                        </a>
                    </li>
                    <li>
                        <a href="http://www.youtube.com/hackneycouncil">
                            <img class="normal" src="http://www.hackney.gov.uk/media/7075/YouTube/normal/youtube" alt="YouTube" />
                            <img class="on-hover" src="http://www.hackney.gov.uk/media/7075/YouTube/highlight/youtube-hov" alt="YouTube" />
                        </a>
                    </li>
                    <li>
                        <a href="http://www.flickr.com/photos/hackneycouncil">
                            <img class="normal" src="http://www.hackney.gov.uk/media/7076/Flickr/normal/flickr" alt="Filickr" />
                            <img class="on-hover" src="http://www.hackney.gov.uk/media/7076/Flickr/highlight/flickr-hov" alt="Filickr" />
                        </a>
                    </li>
                </ul>

            </div>
            <div role="contentinfo" class="copyright">&copy; @DateTime.Now.Year Hackney Council</div>

        </footer>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
</body>
</html>
